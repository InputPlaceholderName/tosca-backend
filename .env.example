# Password for database user
POSTGRES_PASSWORD=test
# Username for database user
POSTGRES_USER=postgres
# Database host
POSTGRES_HOST=localhost
# OIDC base (discovery) URL. (oauth2 url)
OIDC_URL=
OIDC_CLIENT_ID=
OIDC_CLIENT_SECRET=
